{
    "name": "xedi/eloquent-sync",
    "description": "Adds Sync functionality to eloquent relations",
    "type": "library",
    "require": {
        "php": "^7.1.3",
        "illuminate/support": "^5.8|^6.0|^7.0",
        "illuminate/database": "^5.8",
        "symfony/http-foundation": "^4.3.8",
        "symfony/mime": "^4.3.8"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5|^8.0",
        "squizlabs/php_codesniffer": "^3.4",
        "mockery/mockery": "^1.2",
        "orchestra/testbench": "^3.5|^4.0|^5.0",
        "doctrine/dbal": "^2.9"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Chris Smith",
            "email": "chris@xedi.com"
        },
        {
            "name": "Callum Eddisford",
            "email": "callum@xedi.com"
        }
    ],
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "Xedi\\Eloquent\\Sync\\": "lib/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "lib/",
            "tests/"
        ]
    }
}
